<Page x:Class="KDZ_Yagunov.Andrey_MedicalService.DoctorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KDZ_Yagunov.Andrey_MedicalService"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="DoctorPage">

    <Grid>
        <Button x:Name="buttonMain1" Content="Go to previous page" HorizontalAlignment="Left" Height="26" Margin="10,264,0,0" VerticalAlignment="Top" Width="280" Click="buttonMain1_Click"/>
        <Button x:Name="buttonAddDoctor" Content="Add Doctor" HorizontalAlignment="Left" Height="49" Margin="10,210,0,0" VerticalAlignment="Top" Width="132" Click="buttonAddDoctor_Click"/>
        <ListBox x:Name="listBoxDoctorPeople" HorizontalAlignment="Left" Height="195" Margin="10,10,0,0" VerticalAlignment="Top" Width="280"/>
        <Button x:Name="buttonRemoveDoctor" Content="Remove" HorizontalAlignment="Left" Height="49" Margin="156,210,0,0" VerticalAlignment="Top" Width="134"/>

    </Grid>
</Page>
