<Window x:Class="KDZ_Yagunov.Andrey_MedicalService.NewDoctor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KDZ_Yagunov.Andrey_MedicalService"
        mc:Ignorable="d"
        Title="New Doctor" Height="300" Width="300">
    <Grid>
        <Button x:Name="Add new doctor" Content="Add new doctor" HorizontalAlignment="Left" Height="38" Margin="31,207,0,0" VerticalAlignment="Top" Width="233"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="30" Margin="31,109,0,0" TextWrapping="Wrap" Text="Specialty" VerticalAlignment="Top" Width="233"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="30" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="233" Margin="31,63,0,0"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="30" TextWrapping="Wrap" Text="Rating" VerticalAlignment="Top" Width="233" Margin="31,156,0,0"/>

    </Grid>
</Window>
