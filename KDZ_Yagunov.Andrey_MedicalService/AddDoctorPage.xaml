<Page x:Class="KDZ_Yagunov.Andrey_MedicalService.AddDoctorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KDZ_Yagunov.Andrey_MedicalService"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="AddDoctorPage">

    <Grid>
        <TextBox x:Name="textBoxDoctorName" HorizontalAlignment="Left" Height="38" Margin="10,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="271"/>
        <Button x:Name="buttonAddDoctor" Content="addDoctor" HorizontalAlignment="Left" Height="35" Margin="20,192,0,0" VerticalAlignment="Top" Width="157" Click="buttonAddDoctor_Click"/>
        <TextBox x:Name="textBoxDoctorSpecialty" HorizontalAlignment="Left" Height="38" Margin="10,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="271"/>
        <TextBox x:Name="textBoxDoctorRating" HorizontalAlignment="Left" Height="32" Margin="20,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="245" TextChanged="textBoxDoctorRating_TextChanged"/>
        <Button x:Name="buttonBackToDoctorPage" Content="Back!!" HorizontalAlignment="Left" Height="35" Margin="201,192,0,0" VerticalAlignment="Top" Width="80" Click="buttonBackToDoctorPage_Click"/>

    </Grid>
</Page>
