<Window x:Class="KDZ_Yagunov.Andrey_MedicalService.NewMedicaments"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KDZ_Yagunov.Andrey_MedicalService"
        mc:Ignorable="d"
        Title="New Medicaments" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="30" Margin="36,58,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="223"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="30" Margin="36,113,0,0" TextWrapping="Wrap" Text="Type" VerticalAlignment="Top" Width="223"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="30" Margin="36,163,0,0" TextWrapping="Wrap" Text="Quantity" VerticalAlignment="Top" Width="223"/>
        <Button x:Name="button" Content="Add new Medicaments" HorizontalAlignment="Left" Height="30" Margin="36,230,0,0" VerticalAlignment="Top" Width="223"/>

    </Grid>
</Window>
